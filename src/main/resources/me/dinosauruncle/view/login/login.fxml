<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="660.0" prefWidth="430.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.dinosauruncle.controller.LoginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" style="-fx-background-color: #D0ECE7 #D0ECE7;" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" contentDisplay="BOTTOM" layoutX="113.0" layoutY="162.0" style="&#10;" text="공룡삼촌채팅" textAlignment="CENTER" textFill="#70a0ff" wrapText="false">
          <font>
            <Font size="35.0" />
          </font>
        </Label>
            <TextField fx:id="loginId" alignment="CENTER" layoutX="112.0" layoutY="330.0" onKeyPressed="#validation" prefHeight="26.0" prefWidth="206.0" promptText="채팅계정 (이메일 또는 아이디)" />
            <PasswordField fx:id="loginPw" alignment="CENTER" layoutX="112.0" layoutY="380.0" onKeyPressed="#validation" prefHeight="26.0" prefWidth="206.0" promptText="비밀번호" />
            <Button fx:id="loginButton" alignment="CENTER" disable="true" layoutX="112.0" layoutY="421.0" mnemonicParsing="false" onAction="#login" prefHeight="37.0" prefWidth="206.0" style="-fx-background-color: #626567#626567;" text="로그인" textFill="WHITE" textOverrun="CLIP" />
            <Label fx:id="searchId" alignment="CENTER" layoutX="218.0" layoutY="566.0" prefHeight="19.0" prefWidth="53.0" text="계정 찾기" textAlignment="CENTER">
               <font>
                  <Font name="Apple Chancery" size="10.0" />
               </font>
            </Label>
            <Label fx:id="pwReSet" alignment="CENTER" layoutX="171.0" layoutY="603.0" prefHeight="19.0" prefWidth="88.0" text="비밀번호 재설정" textAlignment="CENTER">
               <font>
                  <Font name="Apple Chancery" size="10.0" />
               </font>
            </Label>
            <Label fx:id="createAccount" alignment="CENTER" layoutX="143.0" layoutY="566.0" prefHeight="19.0" prefWidth="65.0" text="계정 만들기" onMouseClicked="#changeScene" textAlignment="CENTER">
               <font>
                  <Font name="Apple Chancery" size="10.0" />
               </font>
            </Label>
            <PasswordField alignment="CENTER" layoutX="113.0" layoutY="376.0" prefHeight="26.0" prefWidth="206.0" promptText="비밀번호" />
      </children>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
    </AnchorPane>
  </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
